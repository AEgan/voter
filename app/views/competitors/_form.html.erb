<%= simple_form_for @competitor, :html => { :multipart => true } do |f| %>
  
  <fieldset>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.hidden_field :wins, :value => 0 %>
    <%= f.hidden_field :times_played, :value => 0 %>
    <%= f.hidden_field :elo, :value => 1200 %>
    <%= f.input :photo %>
  </fieldset>
<% end %>